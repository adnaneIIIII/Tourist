generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model carslists{
  id  Int @id @default(autoincrement())
  carname String
  brand String
  brandyear Int
  price Float
  image String
  cartype String
}

model booking{
  id          Int    @id @default(autoincrement())
  fullname    String
  number      String
  email       String
  pickup      String
  dropoff     String
  carname     String
}


